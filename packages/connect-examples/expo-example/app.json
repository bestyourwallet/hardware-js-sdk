{
  "expo": {
    "name": "HardwareExample",
    "slug": "hardware-example",
    "scheme": "hardware-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "headers": {
      "Permissions-Policy": "usb=(self)"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.onekey.dev.hardware.example"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.onekey.dev.hardware.example"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "webpack",
      "baseUrl": "/expo-example/"
    },
    "plugins": ["expo-image-picker", "expo-document-picker", "expo-localization"]
  }
}
